{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "Path": "Temp/access.log",
      "Append": true,
      "MinLevel": "Information",
      "FileSizeLimitBytes": 0,
      "MaxRollingFiles": 1
    }
  },
  "AllowedHosts": "*"
}
